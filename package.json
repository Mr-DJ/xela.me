{
  "name": "tailwind-nextjs-starter-blog",
  "version": "1.1.4",
  "private": true,
  "scripts": {
    "start": "node ./scripts/next-remote-watch.js ./data",
    "dev": "next dev --webpack",
    "build": "next build && node ./scripts/generate-sitemap && node ./scripts/generate-rss",
    "serve": "next start",
    "analyze": "cross-env ANALYZE=true next build",
    "lint": "next lint",
    "lint:fix": "eslint . --ext .js,.jsx --fix",
    "prepare": "husky install",
    "compose": "node ./scripts/compose.js",
    "generate-sitemap": "node ./scripts/generate-sitemap.js",
    "generate-rss": "node ./scripts/generate-rss.js"
  },
  "dependencies": {
    "@mailchimp/mailchimp_marketing": "^3.0.58",
    "@tailwindcss/forms": "^0.5.7",
    "@tailwindcss/typography": "^0.5.10",
    "autoprefixer": "^10.2.5",
    "esbuild": "^0.27.2",
    "github-slugger": "^1.3.0",
    "gray-matter": "^4.0.2",
    "image-size": "1.0.0",
    "mdx-bundler": "^10.1.1",
    "next": "^16.1.1",
    "next-themes": "^0.2.1",
    "postcss": "^8.3.5",
    "preact": "^10.5.13",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reading-time": "1.3.0",
    "rehype-autolink-headings": "^6.0.0",
    "rehype-katex": "^6.0.0",
    "rehype-prism-plus": "^1.1.0",
    "rehype-slug": "^5.0.0",
    "remark-footnotes": "^4.0.0",
    "remark-gfm": "^2.0.0",
    "remark-math": "^5.0.0",
    "sharp": "^0.32.6",
    "smoothscroll-polyfill": "^0.4.4",
    "tailwindcss": "^3.4.1",
    "unist-util-visit": "^4.0.0"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "^16.1.1",
    "@svgr/webpack": "^8.1.0",
    "cross-env": "^7.0.3",
    "dedent": "^0.7.0",
    "eslint": "^9.0.0",
    "eslint-config-next": "^16.1.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "file-loader": "^6.0.0",
    "globby": "11.0.3",
    "husky": "^6.0.0",
    "inquirer": "^8.1.1",
    "lint-staged": "^11.0.0",
    "next-remote-watch": "^1.0.0",
    "prettier": "^2.4.1",
    "socket.io": "^4.1.3",
    "socket.io-client": "^4.1.3",
    "chalk": "^4.1.2",
    "chokidar": "^3.5.3",
    "commander": "^11.1.0",
    "express": "^4.18.2"
  },
  "lint-staged": {
    "*.+(js|jsx|ts|tsx)": [
      "eslint --fix"
    ],
    "*.+(js|jsx|ts|tsx|json|css|md|mdx)": [
      "prettier --write"
    ]
  }
}
